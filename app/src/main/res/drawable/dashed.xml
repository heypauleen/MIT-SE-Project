<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <shape>
        <solid android:color="#ffffff" />
        <stroke
            android:dashGap="30px"
            android:dashWidth="30px"
            android:width="1dp"
            android:color="#C0BEBF" />
        <padding
            android:bottom="5dp"
            android:left="5dp"
            android:right="5dp"
            android:top="5dp" />
    </shape>
</item>
</layer-list>